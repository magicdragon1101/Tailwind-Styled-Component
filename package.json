        "@typescript-eslint/parser": "^4.7.0",
        "eslint": "^7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.1.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.0.5"
    },
    "dependencies": {
        "react": "^17.0.1",
